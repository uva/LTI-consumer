<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>0.2.5</Version>
        <Authors>UvA</Authors>
        <Description>LTI 1.3 consumer middleware</Description>
        <PackageProjectUrl>https://github.com/UvA/LTI-consumer</PackageProjectUrl>
        <RepositoryUrl>https://github.com/UvA/LTI-consumer</RepositoryUrl>
        <PackageReleaseNotes>made login URL configurable</PackageReleaseNotes>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
        <FrameworkReference Include="Microsoft.AspNetCore.App" />
    </ItemGroup>
    
    <ItemGroup>
      <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="6.29.0" />
    </ItemGroup>

</Project>
